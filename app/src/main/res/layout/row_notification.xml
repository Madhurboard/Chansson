<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="notificationRowModel" type="com.madhursapplication.app.modules.notification.data.model.NotificationRowModel"/>
    </data>
    <LinearLayout android:id="@+id/linearRowrectanglefiftyeight"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <ImageView android:id="@+id/imageRectangleFiftyEight"
            android:layout_height="@dimen/_70pxh"
            android:layout_width="@dimen/_70pxh"
            android:scaleType="fitXY" cornerRadius="@{@dimen/_20pxh}"
            imageRes="@{@drawable/img_rectangle58}"
            tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle58"/>
        <TextView android:id="@+id/txtPageTitleTwo"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_12pxv"
            android:layout_marginStart="@dimen/_13pxh"
            android:layout_marginTop="@dimen/_10pxv"
            android:layout_weight="0.6" android:layout_width="0dp"
            android:text="@{notificationRowModel.txtPageTitleTwo}"
            style="@style/txtDMSansregular17" tools:text="@string/msg_new_mood_just_d"/>
        <TextView android:ellipsize="end" android:id="@+id/txtTimeOne"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_51pxv"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.17" android:layout_width="0dp"
            android:singleLine="true"
            android:text="@{notificationRowModel.txtTimeOne}"
            style="@style/txtDMSansregular13" tools:text="@string/lbl_3min_ago"/>
    </LinearLayout>
</layout>
